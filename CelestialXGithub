ces.RunService.RenderStepped:Connect(Startrainbow);elseif (Configs.Settings.RainbowMethod.Method=="Heartbeat") then RainbowConnection=Configs.Services.RunService.Heartbeat:Connect(Startrainbow);elseif (Configs.Settings.RainbowMethod.Method=="Stepped") then RainbowConnection=Configs.Services.RunService.Stepped:Connect(Startrainbow);end end local character=Configs.Services.playersService.LocalPlayer.Character or Configs.Services.playersService.LocalPlayer.CharacterAdded:Wait() ;local rootPart=character:WaitForChild("HumanoidRootPart");local humanoid=character:WaitForChild("Humanoid");local FlyPlatform;local FlyRaycast=RaycastParams.new();FlyRaycast.FilterType=Enum.RaycastFilterType.Blacklist;FlyRaycast.RespectCanCollide=true;local SpeedRaycast=RaycastParams.new();SpeedRaycast.FilterType=Enum.RaycastFilterType.Blacklist;SpeedRaycast.RespectCanCollide=true;local function sendNotification(title,text,time) if (Configs.Interface.LastText==text) then return;end Configs.Interface.LastText=text;game.StarterGui:SetCore("SendNotification",{Title=title,Text=text,Duration=time});task.wait(1);Configs.Interface.LastText=nil;end local function calculateMoveVector(cameraRelativeMoveVector) local c,s;local _,_,_,R00,R01,R02,_,_,R12,_,_,R22=Configs.Services.Gamecamera.CFrame:GetComponents();if ((R12<1) and (R12> -1)) then c=R22;s=R02;else c=R00;s= -R01 * math.sign(R12) ;end local norm=math.sqrt((c * c) + (s * s) );return Vector3.new(((c * cameraRelativeMoveVector.X) + (s * cameraRelativeMoveVector.Z))/norm ,0,((c * cameraRelativeMoveVector.Z) -(s * cameraRelativeMoveVector.X))/norm );end local networkownerswitch=tick();local isnetworkowner=function(part) local suc,res=pcall(function() return gethiddenproperty(part,"NetworkOwnershipRule");end);if (suc and (res==Enum.NetworkOwnership.Manual)) then sethiddenproperty(part,"NetworkOwnershipRule",Enum.NetworkOwnership.Automatic);networkownerswitch=tick() + 8 ;end return networkownerswitch<=tick() ;end;local function worldToScreenPoint(pos) local screenPos={Configs.Services.Gamecamera:WorldToScreenPoint(pos)};return screenPos[1] -Vector3.new(0,36,0) ,screenPos[1].Z>0 ;end local function worldtoviewportpoint(pos) local viewportPos={Configs.Services.Gamecamera:WorldToViewportPoint(pos)};return viewportPos[1],viewportPos[1].Z>0 ;end local function isFriend(plr,recolor) if recolor then end return isFriend;end local function isTarget(plr) end local function isVulnerable(plr) return plr.Humanoid and (plr.Humanoid.Health>0) and  not plr.Character:FindFirstChildWhichIsA("ForceField") ;end local raycastWallProperties=RaycastParams.new();local function raycastWallCheck(char,checktable) local lplr=Configs.Services.playersService.LocalPlayer;if  not checktable.IgnoreObject then checktable.IgnoreObject=raycastWallProperties;local filter={lplr.Character,Configs.Services.gameCamera};for _,v in pairs(Configs.Character.entityList) do if v.Targetable then table.insert(filter,v.Character);end end for _,v in pairs(checktable.IgnoreTable or {} ) do table.insert(filter,v);end raycastWallProperties.FilterDescendantsInstances=filter;end local direction=char[checktable.AimPart].Position-checktable.Origin ;local ray=workspace:Raycast(checktable.Origin,direction,checktable.IgnoreObject);return  not ray;end local function EntityNearPosition(distance,checktab) checktab=checktab or {} ;if Configs.Character.isAlive then local sortedentities={};for _,v in pairs(Configs.Character.entityList) do if (v.Targetable and isVulnerable(v)) then local playerPosition=v.RootPart.Position;local mag=(Configs.Character.character.HumanoidRootPart.Position-playerPosition).magnitude;if (checktab.Prediction and (mag>distance)) then mag=(Configs.Character.LocalPosition-playerPosition).magnitude;end if (mag<=distance) then table.insert(sortedentities,{entity=v,Magnitude=(v.Target and  -1) or mag });end end end table.sort(sortedentities,function(a,b) return a.Magnitude<b.Magnitude ;end);for _,v in pairs(sortedentities) do if (checktab.WallCheck and  not raycastWallCheck(v.entity,checktab)) then continue;end return v.entity;end end end local function EntityNearMouse(distance,checktab) local lplr=Configs.Services.playersService.LocalPlayer;checktab=checktab or {} ;if Configs.Character.isAlive then local sortedentities={};local mousepos=Configs.Services.UserInputService:GetMouseLocation();for _,v in pairs(Configs.Character.entityList) do if (v.Targetable and isVulnerable(v)) then local vec,vis=worldtoscreenpoint(v[checktab.AimPart].Position);local mag=(mousepos-Vector2.new(vec.X,vec.Y)).magnitude;if (vis and (mag<=distance)) then table.insert(sortedentities,{entity=v,Magnitude=(v.Target and  -1) or mag });end end end table.sort(sortedentities,function(a,b) return a.Magnitude<b.Magnitude ;end);for _,v in pairs(sortedentities) do if (checktab.WallCheck and  not raycastWallCheck(v.entity,checktab)) then continue;end return v.entity;end end end local function AllNearPosition(distance,amount,checktab) local lplr=Configs.Services.playersService.LocalPlayer;local returnedplayer={};local currentamount=0;checktab=checktab or {} ;if Configs.Character.isAlive then local sortedentities={};for _,v in pairs(Configs.Character.entityList) do if (v.Targetable and isVulnerable(v)) then local playerPosition=v.RootPart.Position;local mag=(Configs.Character.character.HumanoidRootPart.Position-playerPosition).magnitude;if (checktab.Prediction and (mag>distance)) then mag=(Configs.Character.LocalPosition-playerPosition).magnitude;end if (mag<=distance) then table.insert(sortedentities,{entity=v,Magnitude=mag});end end end table.sort(sortedentities,function(a,b) return a.Magnitude<b.Magnitude ;end);for _,v in pairs(sortedentities) do if (checktab.WallCheck and  not raycastWallCheck(v.entity,checktab)) then continue;end table.insert(returnedplayer,v.entity);currentamount=currentamount + 1 ;if (currentamount>=amount) then break;end end end return returnedplayer;end local function getPlayerColor(plr) if isFriend(plr,true) then return Color3.fromRGB(255,0,0);end return plr.TeamColor.Color or Color3.new(1,1,1) ;end local function GetCharacter() Configs.Character.character.Head=character:WaitForChild("Head");Configs.Character.character.Humanoid=character:WaitForChild("Humanoid");Configs.Character.character.HumanoidRootPart=character:WaitForChild("HumanoidRootPart");Configs.Character.isAlive=Configs.Character.character.Humanoid.Health>0 ;Configs.Character.character.Humanoid:GetPropertyChangedSignal("Health"):Connect(function() Configs.Character.isAlive=Configs.Character.character.Humanoid.Health>0 ;end);end GetCharacter();local RunLoops={RenderStepTable={},StepTable={},HeartTable={}};do RunLoops.BindToRenderStep=function(self,name,func) if (RunLoops.RenderStepTable[name]==nil) then RunLoops.RenderStepTable[name]=Configs.Services.runService.RenderStepped:Connect(func);end end;RunLoops.UnbindFromRenderStep=function(self,name) if RunLoops.RenderStepTable[name] then RunLoops.RenderStepTable[name]:Disconnect();RunLoops.RenderStepTable[name]=nil;end end;RunLoops.BindToStepped=function(self,name,func) if (RunLoops.StepTable[name]==nil) then RunLoops.StepTable[name]=Configs.Services.runService.Stepped:Connect(func);end end;RunLoops.UnbindFromStepped=function(self,name) if RunLoops.StepTable[name] then RunLoops.StepTable[name]:Disconnect();RunLoops.StepTable[name]=nil;end end;RunLoops.BindToHeartbeat=function(self,name,func) if (RunLoops.HeartTable[name]==nil) then RunLoops.HeartTable[name]=Configs.Services.runService.Heartbeat:Connect(func);end end;RunLoops.UnbindFromHeartbeat=function(self,name) if RunLoops.HeartTable[name] then RunLoops.HeartTable[name]:Disconnect();RunLoops.HeartTable[name]=nil;end end;end function Flyfunction() local lplr=Configs.Services.playersService.LocalPlayer;if (Configs.Settings.FlySettings.Fly.Enabled==true) then local FlyPlatformTick=tick() + 0.2 ;Configs.Settings.FlySettings.w=(Configs.Services.UserInputService:IsKeyDown(Enum.KeyCode.W) and  -1) or 0 ;Configs.Settings.FlySettings.s=(Configs.Services.UserInputService:IsKeyDown(Enum.KeyCode.S) and 1) or 0 ;Configs.Settings.FlySettings.a=(Configs.Services.UserInputService:IsKeyDown(Enum.KeyCode.A) and  -1) or 0 ;Configs.Settings.FlySettings.d=(Configs.Services.UserInputService:IsKeyDown(Enum.KeyCode.D) and 1) or 0 ;table.insert(Configs.Settings.FlySettings.FlyConnections,Configs.Services.UserInputService.InputBegan:Connect(function(input1) if (Configs.Services.UserInputService:GetFocusedTextBox()~=nil) then return;end if (input1.KeyCode==Enum.KeyCode.W) then Configs.Settings.FlySettings.w= -1;elseif (input1.KeyCode==Enum.KeyCode.S) then Configs.Settings.FlySettings.s=1;elseif (input1.KeyCode==Enum.KeyCode.A) then Configs.Settings.FlySettings.a= -1;elseif (input1.KeyCode==Enum.KeyCode.D) then Configs.Settings.FlySettings.d=1;end if (Configs.Settings.FlySettings.FlyVertical.Enabled==true) then local divided=Configs.Settings.FlySettings.FlyKeys.Value:split("/");if (input1.KeyCode==Enum.KeyCode[divided[1]]) then Configs.Settings.FlySettings.FlyUp=true;elseif (input1.KeyCode==Enum.KeyCode[divided[2]]) then Configs.Settings.FlySettings.FlyDown=true;end end end));table.insert(Configs.Settings.FlySettings.FlyConnections,Configs.Services.UserInputService.InputEnded:Connect(function(input1) local divided=Configs.Settings.FlySettings.FlyKeys.Value:split("/");if (input1.KeyCode==Enum.KeyCode.W) then Configs.Settings.FlySettings.w=0;elseif (input1.KeyCode==Enum.KeyCode.S) then Configs.Settings.FlySettings.s=0;elseif (input1.KeyCode==Enum.KeyCode.A) then Configs.Settings.FlySettings.a=0;elseif (input1.KeyCode==Enum.KeyCode.D) then Configs.Settings.FlySettings.d=0;elseif (input1.KeyCode==Enum.KeyCode[divided[1]]) then Configs.Settings.FlySettings.FlyUp=false;elseif (input1.KeyCode==Enum.KeyCode[divided[2]]) then Configs.Settings.FlySettings.FlyDown=false;end end));if Configs.Services.UserInputService.TouchEnabled then pcall(function() local jumpButton=lplr.PlayerGui.TouchGui.TouchControlFrame.JumpButton;table.insert(Configs.Settings.FlySettings.FlyConnections,jumpButton:GetPropertyChangedSignal("ImageRectOffset"):Connect(function() Configs.Settings.FlySettings.FlyUp=jumpButton.ImageRectOffset.X==146 ;end));Configs.Settings.FlySettings.FlyUp=jumpButton.ImageRectOffset.X==146 ;end);end if ((Configs.Settings.FlySettings.FlyMethod.Value=="Jump") and Configs.Character.isAlive) then Configs.Character.character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping);end local FlyTP=false;local FlyTPTick=tick();local FlyTPY;RunLoops:BindToHeartbeat("Fly",function(delta) if (Configs.Character.isAlive and ((typeof(Configs.Character.character.HumanoidRootPart)~="Instance") or isnetworkowner(Configs.Character.character.HumanoidRootPart))) then Configs.Character.character.Humanoid.PlatformStand=Configs.Settings.FlySettings.FlyPlatformStanding.Enabled;if  not Configs.Settings.FlySettings.FlyY then Configs.Settings.FlySettings.FlyY=Configs.Character.character.HumanoidRootPart.CFrame.p.Y;end local movevec=(((Configs.Settings.FlySettings.FlyMoveMethod.Value=="Manual") and calculateMoveVector(Vector3.new(Configs.Settings.FlySettings.a + Configs.Settings.FlySettings.d ,0,Configs.Settings.FlySettings.w + Configs.Settings.FlySettings.s ))) or Configs.Character.character.Humanoid.MoveDirection).Unit;movevec=((movevec==movevec) and Vector3.new(movevec.X,0,movevec.Z)) or Vector3.zero ;if (Configs.Settings.FlySettings.FlyState.Value=="None") then Configs.Character.character.Humanoid:ChangeState(Enum.HumanoidStateType[Configs.Settings.FlySettings.FlyState.Value]);end if ((Configs.Settings.FlySettings.FlyMethod.Value=="Normal") or (Configs.Settings.FlySettings.FlyMethod.Value=="Bounce")) then if Configs.Settings.FlySettings.FlyPlatformStanding.Enabled then Configs.Character.character.HumanoidRootPart.CFrame=CFrame.new(Configs.Character.character.HumanoidRootPart.CFrame.p,Configs.Character.character.HumanoidRootPart.CFrame.p + Configs.Services.Gamecamera.CFrame.lookVector );Configs.Character.character.HumanoidRootPart.RotVelocity=Vector3.zero;end Configs.Character.character.HumanoidRootPart.Velocity=(movevec * Configs.Settings.FlySettings.FlySpeed.Value) + Vector3.new(0,0.85 + (((Configs.Settings.FlySettings.FlyMethod.Value=="Bounce") and ((((tick()%0.5)>0.25) and  -10) or 10)) or 0) + ((Configs.Settings.FlySettings.FlyUp and Configs.Settings.FlySettings.FlyVerticalSpeed.Value) or 0) + ((Configs.Settings.FlySettings.FlyDown and  -Configs.Settings.FlySettings.FlyVerticalSpeed.Value) or 0) ,0) ;else if Configs.Settings.FlySettings.FlyUp then Configs.Settings.FlySettings.FlyY=Configs.Settings.FlySettings.FlyY + (Configs.Settings.FlySettings.FlyVerticalSpeed.Value * delta) ;end if Configs.Settings.FlySettings.FlyDown then Configs.Settings.FlySettings.FlyY=Configs.Settings.FlySettings.FlyY-(Configs.Settings.FlySettings.FlyVerticalSpeed.Value * delta) ;end local newMovementPosition=movevec * math.max(Configs.Settings.FlySettings.FlySpeed.Value-Configs.Character.character.Humanoid.WalkSpeed ,0) * delta ;newMovementPosition=Vector3.new(newMovementPosition.X,Configs.Settings.FlySettings.FlyY-Configs.Character.character.HumanoidRootPart.CFrame.p.Y ,newMovementPosition.Z);if Configs.Settings.FlySettings.FlyWallCheck.Enabled then FlyRaycast.FilterDescendantsInstances={lplr.Character,Configs.Services.Gamecamera};local ray=workspace:Raycast(Configs.Character.character.HumanoidRootPart.Position,newMovementPosition,FlyRaycast);if (ray and ray.Instance.CanCollide) then newMovementPosition=ray.Position-Configs.Character.character.HumanoidRootPart.Position ;Configs.Settings.FlySettings.FlyY=ray.Position.Y;end end local origvelo=Configs.Character.character.HumanoidRootPart.Velocity;if (Configs.Settings.FlySettings.FlyMethod.Value=="CFrame") then Configs.Character.character.HumanoidRootPart.CFrame=Configs.Character.character.HumanoidRootPart.CFrame + newMovementPosition ;if Configs.Settings.FlySettings.FlyCFrameVelocity.Enabled then Configs.Character.character.HumanoidRootPart.Velocity=Vector3.new(origvelo.X,0,origvelo.Z);end if Configs.Settings.FlySettings.FlyPlatformStanding.Enabled then Configs.Character.character.HumanoidRootPart.CFrame=CFrame.new(Configs.Character.character.HumanoidRootPart.CFrame.p,Configs.Character.character.HumanoidRootPart.CFrame.p + Configs.Services.Gamecamera.CFrame.lookVector );end elseif (Configs.Settings.FlySettings.FlyMethod.Value=="Jump") then Configs.Character.character.HumanoidRootPart.CFrame=Configs.Character.character.HumanoidRootPart.CFrame + Vector3.new(newMovementPosition.X,0,newMovementPosition.Z) ;if (Configs.Character.character.HumanoidRootPart.Velocity.Y< -(Configs.Character.character.Humanoid.JumpPower-(((Configs.Settings.FlySettings.FlyUp and Configs.Settings.FlySettings.FlyVerticalSpeed.Value) or 0) -((Configs.Settings.FlySettings.FlyDown and Configs.Settings.FlySettings.FlyVerticalSpeed.Value) or 0)))) then Configs.Settings.FlySettings.FlyJumpCFrame=Configs.Character.character.HumanoidRootPart.CFrame * CFrame.new(0, -Configs.Character.character.Humanoid.HipHeight,0) ;Configs.Character.character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping);end else if (FlyTPTick<=tick()) then FlyTP= not FlyTP;if FlyTP then if FlyTPY then Configs.Settings.FlySettings.FlyY=FlyTPY;end else FlyTPY=Configs.Settings.FlySettings.FlyY;FlyRaycast.FilterDescendantsInstances={lplr.Character,Configs.Services.Gamecamera};local ray=workspace:Raycast(Configs.Character.character.HumanoidRootPart.Position,Vector3.new(0, -10000,0),FlyRaycast);if ray then Configs.Settings.FlySettings.FlyY=ray.Position.Y + (Configs.Character.character.HumanoidRootPart.Size.Y/2) + Configs.Character.character.Humanoid.HipHeight ;end end FlyTPTick=tick() + (((FlyTP and Configs.Settings.FlySettings.FlyTPOn.Value) or Configs.Settings.FlySettings.FlyTPOff.Value)/10) ;end Configs.Character.character.HumanoidRootPart.CFrame=Configs.Character.character.HumanoidRootPart.CFrame + newMovementPosition ;if Configs.Settings.FlySettings.FlyPlatformStanding.Enabled then Configs.Character.character.HumanoidRootPart.CFrame=CFrame.new(Configs.Character.character.HumanoidRootPart.CFrame.p,Configs.Character.character.HumanoidRootPart.CFrame.p + Configs.Services.Gamecamera.CFrame.lookVector );Configs.Character.character.HumanoidRootPart.RotVelocity=Vector3.zero;end end end if Configs.Settings.FlySettings.FlyPlatform then Configs.Settings.FlySettings.FlyPlatform.CFrame=((Configs.Settings.FlySettings.FlyMethod.Value=="Jump") and Configs.Settings.FlySettings.FlyJumpCFrame) or (Configs.Character.character.HumanoidRootPart.CFrame * CFrame.new(0, -(Configs.Character.character.Humanoid.HipHeight + (Configs.Character.character.HumanoidRootPart.Size.Y/2) + 0.53),0)) ;Configs.Settings.FlySettings.FlyPlatform.Parent=Configs.Services.Gamecamera;if (Configs.Settings.FlySettings.FlyUp or (FlyPlatformTick>=tick())) then Configs.Character.character.Humanoid:ChangeState(Enum.HumanoidStateType.Landed);end end else Configs.Settings.FlySettings.FlyY=nil;end end);elseif (Configs.Settings.FlySettings.Fly.Enabled==false) then Configs.Settings.FlySettings.FlyUp=false;Configs.Settings.FlySettings.FlyDown=false;Configs.Settings.FlySettings.FlyY=nil;RunLoops:UnbindFromHeartbeat("Fly");if (Configs.Character.isAlive and Configs.Settings.FlySettings.FlyPlatformStanding.Enabled) then Configs.Character.character.Humanoid.PlatformStand=false;end if Configs.Settings.FlySettings.FlyPlatform then Configs.Settings.FlySettings.FlyPlatform.Parent=nil;Configs.Character.character.Humanoid:ChangeState(Enum.HumanoidStateType.Landed);end end end function SpeedFunction() local lplr=Configs.Services.playersService.LocalPlayer;if (Configs.Character.isAlive and ((typeof(Configs.Character.character.HumanoidRootPart)~="Instance") or isnetworkowner(Configs.Character.character.HumanoidRootPart))) then local movevec;if (Configs.Settings.SpeedSettings.SpeedMoveMethod.Value=="Manual") then movevec=calculateMoveVector(Vector3.new(Configs.Settings.SpeedSettings.a + Configs.Settings.SpeedSettings.d ,0,Configs.Settings.SpeedSettings.w + Configs.Settings.SpeedSettings.s ));else movevec=Configs.Character.character.Humanoid.MoveDirection;end if (movevec and (movevec.Magnitude>0)) then movevec=movevec.Unit;movevec=Vector3.new(movevec.X,0,movevec.Z);else movevec=Vector3.zero;end SpeedRaycast.FilterDescendantsInstances={lplr.Character,Configs.Services.Gamecamera};local w=(Configs.Services.UserInputService:IsKeyDown(Enum.KeyCode.W) and  -1) or 0 ;local s=(Configs.Services.UserInputService:IsKeyDown(Enum.KeyCode.S) and 1) or 0 ;local a=(Configs.Services.UserInputService:IsKeyDown(Enum.KeyCode.A) and  -1) or 0 ;local d=(Configs.Services.UserInputService:IsKeyDown(Enum.KeyCode.D) and 1) or 0 ;table.insert(Configs.Settings.SpeedSettings.SpeedConnections,Configs.Services.UserInputService.InputBegan:Connect(function(input) if (Configs.Services.UserInputService:GetFocusedTextBox()==nil) then if (input.KeyCode==Enum.KeyCode.W) then w= -1;end if (input.KeyCode==Enum.KeyCode.S) then s=1;end if (input.KeyCode==Enum.KeyCode.A) then a= -1;end if (input.KeyCode==Enum.KeyCode.D) then d=1;end end end));table.insert(Configs.Settings.SpeedSettings.SpeedConnections,Configs.Services.UserInputService.InputEnded:Connect(function(input) if (input.KeyCode==Enum.KeyCode.W) then w=0;end if (input.KeyCode==Enum.KeyCode.S) then s=0;end if (input.KeyCode==Enum.KeyCode.A) then a=0;end if (input.KeyCode==Enum.KeyCode.D) then d=0;end end));local pulsetick=tick();task.spawn(function() repeat pulsetick=tick() + (Configs.Settings.SpeedSettings.SpeedPulseDuration.Value/100) ;task.wait((Configs.Settings.SpeedSettings.SpeedDelay.Value/10) + (Configs.Settings.SpeedSettings.SpeedPulseDuration.Value/100) );until  not Configs.Settings.SpeedSettings.Speed.Enabled end);RunLoops:BindToHeartbeat("Speed",function(delta) if (Configs.Character.isAlive and ((typeof(Configs.Character.character.HumanoidRootPart)~="Instance") or isnetworkowner(Configs.Character.character.HumanoidRootPart))) then local movevec=(((Configs.Settings.SpeedSettings.SpeedMoveMethod.Value=="Manual") and calculateMoveVector(Vector3.new(a + d ,0,w + s ))) or Configs.Character.character.Humanoid.MoveDirection).Unit;movevec=((movevec==movevec) and Vector3.new(movevec.X,0,movevec.Z)) or Vector3.zero ;SpeedRaycast.FilterDescendantsInstances={lplr.Character,Configs.Services.Gamecamera};if (Configs.Settings.SpeedSettings.SpeedBypassMethod=="Velocity") then if Configs.Settings.SpeedSettings.SpeedAnimation.Enabled then for _,v in pairs(Configs.Character.character.Humanoid:GetPlayingAnimationTracks()) do if ((v.Name=="WalkAnim") or (v.Name=="RunAnim")) then v:AdjustSpeed(Configs.Character.character.Humanoid.WalkSpeed/16 );end end end local newvelo=movevec * Configs.Settings.SpeedSettings.SpeedValue.Value ;Configs.Character.character.HumanoidRootPart.Velocity=Vector3.new(newvelo.X,Configs.Character.character.HumanoidRootPart.Velocity.Y,newvelo.Z);elseif (Configs.Settings.SpeedSettings.SpeedBypassMethod=="CFrame") then local newpos=movevec * math.max(Configs.Settings.SpeedSettings.SpeedValue.Value-Configs.Character.character.Humanoid.WalkSpeed ,0) * delta ;if Configs.Settings.SpeedSettings.SpeedWallCheck.Enabled then local ray=workspace:Raycast(Configs.Character.character.HumanoidRootPart.Position,newpos,SpeedRaycast);if ray then newpos=ray.Position-Configs.Character.character.HumanoidRootPart.Position ;end end Configs.Character.character.HumanoidRootPart.CFrame=Configs.Character.character.HumanoidRootPart.CFrame + newpos ;elseif (Configs.Settings.SpeedSettings.SpeedBypassMethod=="TP") then if (Configs.Settings.SpeedSettings.SpeedDelayTick<=tick()) then Configs.Settings.SpeedSettings.SpeedDelayTick=tick() + (Configs.Settings.SpeedSettings.SpeedDelay.Value/10) ;local newpos=movevec * Configs.Settings.SpeedSettings.SpeedValue.Value ;if Configs.Settings.SpeedSettings.SpeedWallCheck.Enabled then local ray=workspace:Raycast(Configs.Character.character.HumanoidRootPart.Position,newpos,SpeedRaycast);if ray then newpos=ray.Position-Configs.Character.character.HumanoidRootPart.Position ;end end Configs.Character.character.HumanoidRootPart.CFrame=Configs.Character.character.HumanoidRootPart.CFrame + newpos ;end elseif (Configs.Settings.SpeedSettings.SpeedBypassMethod=="Pulse") then local pulsenum=Configs.Settings.SpeedSettings.SpeedPulseDuration.Value/100 ;local newvelo=movevec * (Configs.Settings.SpeedSettings.SpeedValue.Value + ((Configs.Character.character.Humanoid.WalkSpeed-Configs.Settings.SpeedSettings.SpeedValue.Value) * (1 -(math.max(pulsetick-tick() ,0)/pulsenum)))) ;Configs.Character.character.HumanoidRootPart.Velocity=Vector3.new(newvelo.X,Configs.Character.character.HumanoidRootPart.Velocity.Y,newvelo.Z);elseif (Configs.Settings.SpeedSettings.SpeedBypassMethod=="None") then if  not Configs.Settings.SpeedSettings.oldWalkSpeed then Configs.Settings.SpeedSettings.oldWalkSpeed=Configs.Character.character.Humanoid.WalkSpeed;end Configs.Character.character.Humanoid.WalkSpeed=Configs.Settings.SpeedSettings.SpeedValue.Value;end if (Configs.Settings.SpeedSettings.SpeedJump.Enabled and Configs.Settings.SpeedSettings.SpeedJumpAlways.Enabled) then if ((Configs.Character.character.Humanoid.FloorMaterial~=Enum.Material.Air) and (Configs.Character.character.Humanoid.MoveDirection~=Vector3.zero)) then if Configs.Settings.SpeedSettings.SpeedJumpVanilla.Enabled then Configs.Character.character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping);else Configs.Character.character.HumanoidRootPart.Velocity=Vector3.new(Configs.Character.character.HumanoidRootPart.Velocity.X,Configs.Settings.SpeedSettings.SpeedJumpHeight.Value,Configs.Character.character.HumanoidRootPart.Velocity.Z);end end end end end);end end local function floorESPPosition(pos) return Vector2.new(math.floor(pos.X),math.floor(pos.Y));end local function ESPWorldToViewport(pos) local newpos=worldtoviewportpoint(Configs.Services.Gamecamera.CFrame:pointToWorldSpace(Configs.Services.Gamecamera.CFrame:pointToObjectSpace(pos)));return Vector2.new(newpos.X,newpos.Y);end local function DrawingCheck() if (Drawing==nil) then return "No";else return "Yes";end end local Find_Required=DrawingCheck();function CreateESP() if ((Configs.Settings.ESPSettings.ESP.Enabled==true) and (Find_Required=="Yes")) then local workspace=game:GetService("Workspace");local Autothickness=false;if (Configs.Settings.ESPSettings.DynamicCorner.Enabled==true) then local Space=game:GetService("Workspace");local Player=Configs.Services.playersService.LocalPlayer;local Camera=Space.CurrentCamera;local function NewLine(color,thickness) local line=Drawing.new("Line");line.Visible=false;line.From=Vector2.new(0,0);line.To=Vector2.new(0,0);line.Color=color;line.Thickness=thickness;line.Transparency=1;return line;end local function Vis(lib,state) for i,v in pairs(lib) do v.Visible=state;end end local function Colorize(lib,color) for i,v in pairs(lib) do v.Color=color;end end local Black=Color3.fromRGB(0,0,0);local function Rainbow(lib,delay) Colorize(lib,Configs.Settings.ESPSettings.DynamicCornerColor);wait(delay);Rainbow(lib);end local function Main(plr) repeat wait();until (plr.Character~=nil) and (plr.Character:FindFirstChild("Humanoid")~=nil)  local R15;if (plr.Character.Humanoid.RigType==Enum.HumanoidRigType.R15) then R15=true;else R15=false;end local Library={TL1=NewLine(Configs.Settings.ESPSettings.DynamicCornerColor,Configs.Settings.ESPSettings.Box_Thickness),TL2=NewLine(Configs.Settings.ESPSettings.DynamicCornerColor,Configs.Settings.ESPSettings.Box_Thickness),TR1=NewLine(Configs.Settings.ESPSettings.DynamicCornerColor,Configs.Settings.ESPSettings.Box_Thickness),TR2=NewLine(Configs.Settings.ESPSettings.DynamicCornerColor,Configs.Settings.ESPSettings.Box_Thickness),BL1=NewLine(Configs.Settings.ESPSettings.DynamicCornerColor,Configs.Settings.ESPSettings.Box_Thickness),BL2=NewLine(Configs.Settings.ESPSettings.DynamicCornerColor,Configs.Settings.ESPSettings.Box_Thickness),BR1=NewLine(Configs.Settings.ESPSettings.DynamicCorn